{
  "name": "unframe_ai",
  "version": "1.0.0",
  "description": "Full-Stack AI-Powered Drive/Gmail/Trello App",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm --filter=./backend dev\" \"pnpm --filter=./frontend dev\"",
    "build": "pnpm --filter=./backend build && pnpm --filter=./frontend build",
    "test": "concurrently \"pnpm --filter=./backend test\" \"pnpm --filter=./frontend test\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,html}\""
  },
  "keywords": [
    "ai",
    "drive",
    "gmail",
    "trello",
    "full-stack"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "^3.2.5"
  }
}
